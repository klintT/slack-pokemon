{
  "name": "node-js-sample",
  "version": "0.1.0",
  "description": "A sample Node.js app using Express 4",
  "main": "index.js",
  "scripts": {
    "start": "if [ \"`pm2 id \"Slack Pokemon\"`\" == \"[]\" ]; then pm2 start index.js --name=\"Slack Pokemon\"; else pm2 restart --update-env \"Slack Pokemon\"; fi",
    "restart": "pm2 restart \"Slack Pokemon\""
  },
  "dependencies": {
    "body-parser": "*",
    "express": ">=4.0.0",
    "q": ">=1.0.1",
    "redis": ">= 0.10.3",
    "request": "*"
  },
  "engines": {
    "node": "9.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heroku/node-js-sample"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
